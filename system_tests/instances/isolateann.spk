# Load CreateAxes from files 
CreateAxes[name = ax_001d, file = '../axes/1d_v25_m5to10.axb']
CreateAxes[name = ax_002d, file = '../axes/2d_v25_m5to10x2.axb']
CreateAxes[name = ax_003d, file = '../axes/3d_v10_0to10x2_v2_0to5.axb'] 
CreateAxes[name = ax_005d, file = '../axes/5d_v5_m2to2x5.axb']
CreateAxes[name = ax_010d,
  file = '../axes/10d_v5_m10tom5x3_f3_f4_f5_v5_2to4x2_f0_f0.axb']

# Generate data from CreateAxes using test functions
GridSample[name = sumof_001d, axes = ax_001d,
	   test_function = sumofall]
GridSample[name = sumof_002d, axes = ax_002d,
	   test_function = sumofall]
GridSample[name = sumof_003d, axes = ax_003d,
	   test_function = sumofall]
GridSample[name = sumof_005d, axes = ax_005d,
	   test_function = sumofall]
GridSample[name = sumof_010d, axes = ax_010d,
	   test_function = sumofall]


CreateSurface[name = ann_010d, data = sumof_010d, type = ann]

# Evaluate surfaces 
Evaluate[surface = ann_010d, data = sumof_010d]

## Compute surface fitness metrics
Fitness[surface = ann_010d, data = sumof_010d, metric = mean_squared]

# Save data to file
Save[data = sumof_010d, file = '../tmp_data/sumof_010d.spd']
Save[surface = ann_010d, file = '../tmp_surf/ann_sumof_010d.sps']


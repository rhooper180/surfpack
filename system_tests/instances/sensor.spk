# Load data from files
LoadData[name = sensor_data, file = '../perm_data/sensor_1d.txt']

# Create a test data set
AxesBounds[name = ax_test, bounds = '1 | v 0 3 50']
GridPoints[name = sensor_test, def = ax_test]

# Create surfaces using existing data sets
CreateSurface[name = poly0_sensor, data = sensor_data, type = 'Polynomial',
 	      order = 0]
CreateSurface[name = poly1_sensor, data = sensor_data, type = 'Polynomial',
 	      order = 1]
CreateSurface[name = poly2_sensor, data = sensor_data, type = 'Polynomial',
 	      order = 2]
CreateSurface[name = poly3_sensor, data = sensor_data, type = 'Polynomial',
 	      order = 3]
CreateSurface[name = krig_sensor, data = sensor_data, type = 'Kriging']
CreateSurface[name = mars_sensor, data = sensor_data, type = 'Mars']
CreateSurface[name = ann_sensor, data = sensor_data, type = 'ANN']
CreateSurface[name = rbf_sensor, data = sensor_data, type = 'RBFNet']

# Evaluate surfaces 
Evaluate[surface = poly0_sensor, input_data = sensor_test]
Evaluate[surface = poly1_sensor, input_data = sensor_test]
Evaluate[surface = poly2_sensor, input_data = sensor_test]
Evaluate[surface = poly3_sensor, input_data = sensor_test]
Evaluate[surface = krig_sensor, input_data = sensor_test]
Evaluate[surface = mars_sensor, input_data = sensor_test]
Evaluate[surface = ann_sensor, input_data = sensor_test]
Evaluate[surface = rbf_sensor, input_data = sensor_test]

# Compute surface fitness metrics
Fitness[surface = poly0_sensor, metric = 'press']
Fitness[surface = poly1_sensor, metric = 'press']
Fitness[surface = poly2_sensor, metric = 'press']
Fitness[surface = poly3_sensor, metric = 'press']
Fitness[surface = krig_sensor, metric = 'press']
Fitness[surface = mars_sensor, metric = 'press']
Fitness[surface = ann_sensor, metric = 'press']
Fitness[surface = rbf_sensor, metric = 'press']

# Save data to file
SaveData[data = sensor_test, file = '../tmp_data/sensor_test.txt']

# Save surfaces to file
SaveSurface[surface = poly0_sensor, file = '../tmp_surf/poly0_sensor.txt']
SaveSurface[surface = poly1_sensor, file = '../tmp_surf/poly1_sensor.txt']
SaveSurface[surface = poly2_sensor, file = '../tmp_surf/poly2_sensor.txt']
SaveSurface[surface = poly3_sensor, file = '../tmp_surf/poly3_sensor.txt']
SaveSurface[surface = krig_sensor, file = '../tmp_surf/krig_sensor.txt']
SaveSurface[surface = mars_sensor, file = '../tmp_surf/mars_sensor.txt']
SaveSurface[surface = ann_sensor, file = '../tmp_surf/ann_sensor.txt']
SaveSurface[surface = rbf_sensor, file = '../tmp_surf/rbf_sensor.txt']

